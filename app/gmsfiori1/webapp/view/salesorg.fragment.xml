<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">

    <SelectDialog
        id="salesorg"
        noDataText="No Products Found"
        title="Select sales org"
        search="onValueHelpSearchsaleorg"
        confirm="onValueHelpClosesaleorg"
        cancel="onValueHelpClosesaleorg"
        items="{/Sales_OrgSet}">
        <StandardListItem id="_IdardListItem"
            title="{Vkorg}"
            description="{Vkorg}"
       
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>