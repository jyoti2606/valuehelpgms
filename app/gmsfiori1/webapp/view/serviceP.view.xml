<mvc:View
    controllerName="gmsfiori.controller.serviceP"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m" xmlns:core="sap.ui.core"
>
     <ScrollContainer
        id="_IDGenScrollContainer1"
        height="100%"
        width="100%"
        vertical="true"
        focusable="true"
    >
        <Shell id="shell">
            <App id="app">
                <pages>
                    <Page
                        id="page"
                        title="{i18n>title}"
                        enableScrolling="true"
                        showHeader="false"
                    >
                        <content>
                            <Button
                                id="_IDGenButton1"
                                text="Create"
                                press="onCreate"
                            />
                            <Table
                                items="{mainModel>/ServiceCatalogue}"
                                id="table0"
                                selectionChange="onSelect"
                            >
                                <items>
                                    <ColumnListItem
                                        type="Active"
                                        id="item0"
                                    >
                                        <cells>
                                            <Text
                                                id="id1"
                                                text="{mainModel>ServiceParameter}"
                                            />
                                            <Text
                                                id="id2"
                                                text="{mainModel>id}"
                                            />
                                            <Input
                                                id="_IDGenInput1"
                                                width="60px"
                                            />
                                            <CheckBox id="_IDGenCheckBox9" select="onCheckboxSelect1" />
                                            <CheckBox id="_IDGenCheckBox1" select="onCheckboxSelect2"/>
                                            <CheckBox id="_IDGenCheckBox2" select="onCheckboxSelect3"/>
                                            <CheckBox id="_IDGenCheckBox3" select="onCheckboxSelect4"/>
                                            <CheckBox id="_IDGenCheckBox4" select="onCheckboxSelect5"/>
                                            <CheckBox id="_IDGenCheckBox5" select="onCheckboxSelect6"/>
                                            <CheckBox id="_IDGenCheckBox6" select="onCheckboxSelect7"/>
                                            <CheckBox id="_IDGenCheckBox7" select="onCheckboxSelect8"/>
                                            <CheckBox id="_IDGenCheckBox8" select="onCheckboxSelect9"/>
                                        </cells>
                                    </ColumnListItem>
                                </items>
                                <columns>
                                   
                                    <Column
                                        id="_IDGenColumn2"
                                        width="7rem"
                                    >
                                        <header>
                                            <Label
                                                id="_IDGenLabel7"
                                                text="Service Parameter"
                                            />
                                        </header>
                                    </Column>
                                    <Column
                                        id="_IDGenColu"
                                        width="2rem"
                                    >
                                        <header>
                                            <Label
                                                id="_IDGenLl12"
                                                text="ID"
                                            />
                                        </header>
                                    </Column>
                                    <Column
                                        id="_IDGenColum"
                                        width="5rem"
                                    >
                                        <header>
                                            <Label
                                                id="_IDGenLabe"
                                                text="Parent ID"
                                            />
                                        </header>
                                    </Column>
                                    <Column
                                        id="_IDGenColun3"
                                        width="7rem"
                                    >
                                        <header>
                                            <Label
                                                id="_IDGenLab7"
                                                text="Contract Relevant"
                                            />
                                        </header>
                                    </Column>
                                    <Column
                                        id="_IDGenColumn3"
                                        width="7rem"
                                    >
                                        <header>
                                            <Label
                                                id="_IDGenLabel8"
                                                text="Quantity Parameter"
                                            />
                                        </header>
                                    </Column>
                                    <Column
                                        id="_IDGenColumn4"
                                        width="7rem"
                                    >
                                        <header>
                                            <Label
                                                id="_IDGenLabel9"
                                                text="Threshold Relevance"
                                            />
                                        </header>
                                    </Column>
                                    <Column
                                        id="_IDGenColumn5"
                                        width="8rem"
                                    >
                                        <header>
                                            <Label
                                                id="_IDGenLabel10"
                                                text="Referrence Relevant"
                                            />
                                        </header>
                                    </Column>
                                    <Column
                                        id="_IDGenColumn6"
                                        width="8rem"
                                    >
                                        <header>
                                            <Label
                                                id="_IDGenLabel11"
                                                text="Nomination Relevant"
                                            />
                                        </header>
                                    </Column>
                                    <Column
                                        id="_IDGenColumn7"
                                        width="8rem"
                                    >
                                        <header>
                                            <Label
                                                id="_IDGenLabel12"
                                                text="Balancing Relevant"
                                            />
                                        </header>
                                    </Column>
                                    <Column
                                        id="_IDGenColumn8"
                                        width="8rem"
                                    >
                                        <header>
                                            <Label
                                                id="_IDGenLabel13"
                                                text="Allocation Relevant"
                                            />
                                        </header>
                                    </Column>
                                    <Column
                                        id="_IDGenColumn9"
                                        width="6rem"
                                    >
                                        <header>
                                            <Label
                                                id="_IDGenLabel14"
                                                text="Billing Relevant"
                                            />
                                        </header>
                                    </Column>
                                    <Column
                                        id="_IDGenColumn10"
                                        width="6rem"
                                    >
                                        <header>
                                            <Label
                                                id="_IDGenLabel15"
                                                text="Price Relevant"
                                            />
                                        </header>
                                    </Column>
                                </columns>
                            </Table>
                            <Button
                                id="_IDGenButton2"
                                text="Submit"
                                press="onSubmit"
                            />
                        </content>
                    </Page>
                </pages>
            </App>
        </Shell>
    </ScrollContainer>
    <Dialog
        id="createDialog"
        title="Create Field"
        contentWidth="300px"
    >
        <Input
            id="newFieldNameInput"
            placeholder="Enter Field Name"
        />
        <Label
            id="_IDGenLabel16"
            text="Select Field Type :"
        />
        <Select
            id="fieldTypeComboBox"
            change="onComboBoxChange"
            width="200px"
           
        >
            <items>
            <core:Item
                    id="idItem"
 
                />
               
                <core:Item
                    id="_IDGenItem1"
                    key="String"
                    text="String"
                />
                <core:Item
                    id="_IDGenItem2"
                    key="Integer"
                    text="Integer"
                />
                <core:Item
                    id="_IDGenItem3"
                    key="Boolean"
                    text="Boolean"
                />
                <core:Item
                    id="_IDGenItem4"
                    key="Decimal"
                    text="Decimal"
                />
                <core:Item
                    id="_IDGenItem5"
                    key="Date"
                    text="Date"
                />
                <core:Item
                    id="_IDGenItem6"
                    key="Date and Time"
                    text="Date and Time"
                />
                <core:Item
                    id="_IDGenItem7"
                    key="TimeStrapr"
                    text="TimeStrap"
                />
                <core:Item
                    id="_IDGenItem8"
                    key="Table"
                    text="Table"
                />
               
            </items>
        </Select>
        <Input
            id="newFieldNameInput1"
            placeholder="Enter Length"
            visible="false"
        />
        <Button
            id="_IDGenButton3"
            text="Create"
            press="onCreateField"
        />
        <Button
            id="_IDGenButton4"
            text="Cancel"
            press="onCancelCreateField"
        />
    </Dialog>
    <Dialog
    id="profileNameDialog"
    title="Enter Profile Name"
    contentWidth="300px"
>
    <Input
        id="profileNameInput"
        placeholder="Enter Profile Name"
    />
    <Button
        id="_IDGenButtonSubmitProfile"
        text="Submit"
        press="onSubmitProfile"
    />
    <Button
        id="_IDGenButtonCancelProfile"
        text="Cancel"
        press="onCancelProfile"
    />
</Dialog>
</mvc:View>